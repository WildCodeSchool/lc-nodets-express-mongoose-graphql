generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider           = "typegraphql-prisma"
  output             = "../prisma/generated/type-graphql"
  emitTranspiledCode = "true"
}

datasource db {
  provider = "sqlite"
  url      = "file:wilderDatabase.db"
}

model Wilder {
  id     Int      @id
  name   String?
  city   String?
  Skills Skills[]
}

model Skills {
  id       Int     @id
  votes    String?
  title    String?
  wilderId Int
  Wilder   Wilder  @relation(fields: [wilderId], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
